        <SolidColorBrush x:Key="MainColor" Color="#2fc22f"></SolidColorBrush>
        <SolidColorBrush x:Key="ContentText" Color="#505472"></SolidColorBrush>
        <SolidColorBrush x:Key="AccentColor" Color="#0f9ceb" ></SolidColorBrush>
        <Style x:Key="SimpleTextBox" TargetType="TextBox">
            <Setter Property="Foreground" Value="{StaticResource ContentText}"></Setter>
            <Setter Property="FontSize" Value="35"></Setter>
            <Setter Property="FontFamily" Value="Cambria"></Setter>
            <Setter Property="BorderBrush" Value="{StaticResource AccentColor}"></Setter>
        </Style>
        <Style x:Key="SimpleButton" TargetType="Button">
            <Setter Property="Foreground" Value="{StaticResource MainColor}"></Setter>
            <Setter Property="Background" Value="{StaticResource ContentText}"></Setter>
            <Setter Property="FontSize" Value="22"></Setter>
            <Setter Property="FontFamily" Value="Cambria"></Setter>
            <Setter Property="Padding" Value="5"/>
        </Style>
        <Style x:Key="MiddleTextBlock" TargetType="TextBlock">
            <Setter Property="FontSize" Value="45"></Setter>
            <Setter Property="FontFamily" Value="Cambria"></Setter>
            <Setter Property="Foreground" Value="{StaticResource ContentText}"></Setter>

        </Style>
        <Style x:Key="SmallTextBlock" TargetType="TextBlock">
            <Setter Property="FontSize" Value="15"></Setter>
            <Setter Property="FontFamily" Value="Cambria"></Setter>
            <Setter Property="Foreground" Value="{StaticResource ContentText}"></Setter>
        </Style>
        <Style x:Key="SmallMiddleTextBlock" TargetType="TextBlock">
            <Setter Property="FontSize" Value="25"></Setter>
            <Setter Property="FontFamily" Value="Cambria"></Setter>
            <Setter Property="Foreground" Value="{StaticResource ContentText}"></Setter>
        </Style>
        <Style x:Key="BigTextBlock" TargetType="TextBlock">
            <Setter Property="FontSize" Value="60"></Setter>
            <Setter Property="FontFamily" Value="Cambria"></Setter>
            <Setter Property="Foreground" Value="{StaticResource ContentText}"></Setter>
        </Style>
        <BitmapImage x:Key="DefoltImage" UriSource="Images/profile-placeholder.png"></BitmapImage>
        <BitmapImage x:Key="TestImagePlaceholder" UriSource="Images/profiler.jpg"></BitmapImage>
        <class:ProccentTestConvertor x:Key="ProcentConvertor"></class:ProccentTestConvertor>
        <class:ListOneAnswer x:Key="ListOneAnswer"></class:ListOneAnswer>
        <class:ListManyAnswer x:Key="ListManyAnswer"></class:ListManyAnswer>
        <class:OneAnswer x:Key="OneAnswer"></class:OneAnswer>
